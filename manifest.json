{
    "manifest_version": 3,
    "name": "Gitmeow",
    "version": "0.2.0",
    "description": "Gitmeow",
    "permissions": [
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["https://github.com/*", "https://gist.github.com/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "options_page": "options.html",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }